<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="inherited_res_users_view_form">
    <field name="name">inherited res users view form</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_form"/>
    <field name="arch" type="xml">
      <xpath expr="//notebook[last()]" position="inside">
        <page string="Helpdesk Teams" name="helpdesk_team">
          <group>
            <field name="helpdesk_team_ids" widget="many2many_tags"/>
          </group>
        </page>
      </xpath>
    </field>
  </record>

</odoo>
